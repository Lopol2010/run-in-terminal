{
    "name": "run-in-terminal",
    "displayName": "Run in Terminal",
    "description": "Run arbitrary commands (with arguments) in integrated Terminal.",
    "version": "0.0.1",
    "publisher": "kortina",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:runInTerminal.run",
        "onCommand:runInTerminal.runLast"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "runInTerminal.run",
                "title": "Run in Terminal"
            },
            {
                "command": "runInTerminal.runLast",
                "title": "Re-Run Last Command in Terminal"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "eslint": "^4.10.0",
        "eslint-plugin-typescript": "^0.8.0",
        "typescript-eslint-parser": "^8.0.1",
        "typescript": "~2.5.1"
    }
}
